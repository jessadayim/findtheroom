{% extends 'FTRWebBundle:Layout:Layout.html.twig' %}
{% block title %}
<!-- รายละเอียด site -->
    <title>{{ siteTitle }}</title>
    <meta name="description" content="{{ siteDesc }}" />
    <meta name="keyword" content="{{ siteKeyword }}"/>
    <meta name="author" content="{{ siteAuthor }}"/>
    <meta name="copyright" content="{{ siteCopyRight }}"/>
    <meta name="robot" content="{{ siteRobot }}"/>
    <meta name="revisit-after" content="{{ siteRevisitAfter }}"/>
    <meta name="distribution" content="{{ siteDistribution }}"/>

    <!-- facebook tag -->
    <meta property="og:title" content="{{ siteTitle }}"/>
    <meta property="og:type" content="website"/>
    <meta property="og:url" content="{{ siteUrl }}"/>
    <meta property="og:image" content="{{ siteImage }}"/>
    <meta property="og:site_name" content="FindTheRoom"/>
    <meta property="fb:admins" content="USER_ID"/>
    <meta property="og:description" content="{{ siteDesc }}"/>
<!-- #รายละเอียด site -->
{% endblock %}
{% block body %}
<link rel="stylesheet" type="text/css" href="{{asset('css/paging.css')}}">
<script src="{{asset('js/paging/jquery.paginate.js')}}"></script>
<script type="text/javascript" src="{{asset('js/fancybox/jquery.fancybox-1.3.4.pack.js')}}"></script>
<script type="text/javascript" src="{{asset('js/fancybox/jquery.easing-1.4.pack.js')}}"></script>
<link rel="stylesheet" type="text/css" href="{{asset('css/jquery.fancybox-1.3.4.css')}}" media="screen" />
<script type="text/javascript">
		$(function() {
			$("#demo5").paginate({
				count 		: 30,
				start 		: 1,
				display     : 20,
				border					: false,
				text_color  			: '#423C3B',	
				text_hover_color  		: '#423C3B', 
				images					: false,
				mouse					: 'press'
			});
		});
</script>
<script type="text/javascript">
		$(document).ready(function() {
            if('{{enable}}' == true){
                $.fancybox({
                    'content' : "{{path('FTRWebBundle_regis_alertemail')}}",
                    'width' : 700,
                    'height' : 400,
                    'autoScale' : false,
                    'transitionIn' : 'elastic',
                    'transitionOut' : 'elastic',
                    'type' : 'iframe'
                });
            }

			$("a#gmap").fancybox({
				'overlayShow'	: true,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});
			$("a#imap").fancybox({
				'overlayShow'	: true,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});
			$("a#cmap").fancybox({
				'overlayShow'	: true,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});
			$("a#login").fancybox({
				'width'				: 700,
				'height'			: 420,
				'autoScale'			: false,
				'transitionIn'		: 'elastic',
				'transitionOut'		: 'elastic',
				'type'				: 'iframe',
			});
		});
</script>
