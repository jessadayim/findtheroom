<script type="text/javascript">
		$(function() {
			$("#demo5").paginate({
				count 		: 30,
				start 		: 1,
				display     : 20,
				border					: false,
				text_color  			: '#423C3B',	
				text_hover_color  		: '#423C3B', 
				images					: false,
				mouse					: 'press'
			});
		});
</script>
<script type="text/javascript">
		$(document).ready(function() {
			$("a#gmap").fancybox({
				'overlayShow'	: true,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});
			$("a#imap").fancybox({
				'overlayShow'	: true,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});
			$("a#cmap").fancybox({
				'overlayShow'	: true,
				'transitionIn'	: 'elastic',
				'transitionOut'	: 'elastic'
			});
			$("a#login").fancybox({
				'width'				: 700,
				'height'			: 420,
				'autoScale'			: false,
				'transitionIn'		: 'elastic',
				'transitionOut'		: 'elastic',
				'type'				: 'iframe',
			});
		});
</script>
