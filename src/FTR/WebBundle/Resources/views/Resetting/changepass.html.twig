			'content'			: "{{path('TRWebBundle_reset')}}",
			'width'				: 700,
			'height'			: 400,
			'autoScale'			: false,
			'transitionIn'		: 'elastic',
			'transitionOut'		: 'elastic',
			'type'				: 'iframe'
		});
    });
</script>
{% endblock %}