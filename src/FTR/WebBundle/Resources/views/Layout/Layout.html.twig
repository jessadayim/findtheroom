								parent.location.href = "{{path('userbuilding')}}";
								//exit();
							} else if (data == 2) {

								$.fancybox({
									'content' : "{{path('FTRWebBundle_loginpublish', { 'message': '2'})}}",
									'width' : 700,
									'height' : 420,
									'autoScale' : false,
									'transitionIn' : 'elastic',
									'transitionOut' : 'elastic',
									'type' : 'iframe'
								});
							}
						},
						error : function(e) {
							$("#txterror").html("กรุณาตรวจสอบ ชื่อผู้ใช้งาน และรหัสผ่าน");
						}
					});
				});
			});
