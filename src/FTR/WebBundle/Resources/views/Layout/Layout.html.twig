<script>	
	var url = window.location.href,check=false;
	var urlparts = url.split("/");
	for(i = 0; i < urlparts.length; i++){
		if (urlparts[i]=='search'){$("#searchpage").addClass("selected");check=true}
		else if (urlparts[i]=='special/beauty'){$("#").addClass("selected");check=true}
		else if (urlparts[i]=='publish'){$("#publishpage").addClass("selected");check=true}
		else if (urlparts[i]=='regis'){$("#publishpage").addClass("selected");check=true}
		else if (urlparts[i]=='contact'){$("#contactpage").addClass("selected");check=true}
	}
	if(check==false)	$("#mainpage").addClass("selected");

</script>
