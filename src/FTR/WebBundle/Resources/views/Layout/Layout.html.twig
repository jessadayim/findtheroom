		<script language="javascript">
			$(document).ready(function() {
				var mybrowser = navigator.userAgent;
				if (mybrowser.indexOf('Firefox') > 0) {
					$(".login").css("marginLeft", "5px");
					$(".head hr").css("marginTop", "5px");
				}
				if (mybrowser.indexOf('MSIE') > 0) {
					$(".login").css("marginLeft", "0px");
					$("#fie").width("190");
				}

				$("input#log2").click(function(e) {
					//alert("working1");
					$.ajax({
						url : "{{path('FTRWebBundle_login')}}",
						type : 'POST',
						data : "username=" + $('input#username').val() + "&" + "password=" + $('input#password').val(),
						success : function(data) {
							if (data == 0) {
								$.fancybox({
									'content' : "{{path('FTRWebBundle_loginpublish', { 'message': '0'})}}",
									'width' : 700,
									'height' : 420,
