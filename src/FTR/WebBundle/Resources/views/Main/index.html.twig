<div id="update">
    <img src="{{asset('images/update.png')}}"/> <span class="time">( {{ last_update }} )</span>
    <hr class="line" style="top: 60px;"/>
    <table>
        <thead>
        <tr>
            <th width="250">ชื่อหอพัก</th>
            <th width="150">ประเภท</th>
            <th width="150">ชนิด</th>
            <th width="200">โซน/อำเภอ</th>
            <th width="150">ราคา</th>
        </tr>
        </thead>
        <tbody>

        {% for tlist in top_last_building %}
        <tr>
            <td class="first"><a href="{{ path('FTRWebBundle_detail',{'buildId':tlist.id,'province':tlist.PROVINCE_NAME,'prefecture':tlist.AMPHUR_NAME,'slug':tlist.slug}) }}" target="_blank">{{ tlist.building_name }}</a></td>
            <td>{{ tlist.b_type_name }}</td>
            <td>
                {{ tlist.pay_type_name }}
                {#รายเดือน#}
            </td>
            {% if tlist.PROVINCE_NAME !='กรุงเทพมหานคร' %}
            <td>{{ tlist.AMPHUR_NAME }}</td>
            {% else %}
            <td>
                {#{{ tlist.zonename }}#}
                {{ tlist.PROVINCE_NAME }}
            </td>
            {% endif %}
            <td>{{ tlist.start_price }} - {{ tlist.end_price }} บาท</td>
        </tr>
        {% endfor %}

        </tbody>
    </table>
</div>
