<h1>User_level</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Level_name</th>
            <td>{{ entity.levelname }}</td>
        </tr>
        <tr>
            <th>Level_type</th>
            <td>{{ entity.leveltype }}</td>
        </tr>
        <tr>
            <th>Is_enabled</th>
            <td>{{ entity.isenabled }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('user_level') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('user_level_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('user_level_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
