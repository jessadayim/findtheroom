<h1>Facilitylist list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Facility_name</th>
            <th>Facility_type</th>
            <th>Display</th>
            <th>Deleted</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td><a href="{{ path('facilitylist_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
            <td>{{ entity.facilityname }}</td>
            <td>{{ entity.facilitytype }}</td>
            <td>{{ entity.display }}</td>
            <td>{{ entity.deleted }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('facilitylist_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('facilitylist_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('facilitylist_new') }}">
            Create a new entry
        </a>
    </li>
</ul>
