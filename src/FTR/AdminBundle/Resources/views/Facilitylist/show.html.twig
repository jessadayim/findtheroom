<h1>Facilitylist</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Facility_name</th>
            <td>{{ entity.facilityname }}</td>
        </tr>
        <tr>
            <th>Facility_type</th>
            <td>{{ entity.facilitytype }}</td>
        </tr>
        <tr>
            <th>Display</th>
            <td>{{ entity.display }}</td>
        </tr>
        <tr>
            <th>Deleted</th>
            <td>{{ entity.deleted }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('facilitylist') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('facilitylist_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('facilitylist_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
