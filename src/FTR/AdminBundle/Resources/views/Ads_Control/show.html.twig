<h1>Ads_Control</h1>

<table class="record_properties">
    <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Title</th>
            <td>{{ entity.title }}</td>
        </tr>
        <tr>
            <th>Zone</th>
            <td>{{ entity.zone }}</td>
        </tr>
        <tr>
            <th>Codes</th>
            <td>{{ entity.codes }}</td>
        </tr>
        <tr>
            <th>Publish</th>
            <td>{{ entity.publish }}</td>
        </tr>
        <tr>
            <th>Date_start</th>
            <td>{{ entity.datestart|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Date_end</th>
            <td>{{ entity.dateend|date('Y-m-d H:i:s') }}</td>
        </tr>
    </tbody>
</table>

<ul class="record_actions">
    <li>
        <a href="{{ path('ads_control') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('ads_control_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('ads_control_delete', { 'id': entity.id }) }}" method="post">
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
