{% extends 'FTRAdminBundle:Layout:panel_layout.html.twig' %}

{% block body %}
<div id="wrap" class="container_12">
	<h1>Building_site creation</h1>

	<form action="{{ path('building_site_create') }}" method="post" {{ form_enctype(form) }}>
		{{ form_widget(form) }}
		<p>
			<button type="submit">
				Create
			</button>
		</p>
	</form>

	<ul class="record_actions">
		<li>
			<a href="{{ path('building_site') }}"> Back to the list </a>
		</li>
	</ul>
</div>
{% endblock %}