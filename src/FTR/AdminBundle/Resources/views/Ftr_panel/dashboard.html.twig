{% extends 'FTRAdminBundle:Layout:panel_layout.html.twig' %}

{% block body %}
  <!-- ***** Jquery Chart ***** -->
  <script type="text/javascript">
		$(function () {
		    var chart;
		    $(document).ready(function() {
		        chart = new Highcharts.Chart({
		            chart: {
		                renderTo: 'chartcontainer',
		                type: 'line',
		                marginRight: 130,
		                marginBottom: 25
		            },
		            title: {
		                text: 'Monthly Average Temperature',
		                x: -20 //center
		            },
		            subtitle: {
		                text: 'Source: WorldClimate.com',
		                x: -20
		            },
		            xAxis: {
		                categories: ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun',
		                    'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
		            },
		            yAxis: {
		                title: {
		                    text: 'Temperature (°C)'
		                },
		                plotLines: [{
		                    value: 0,
		                    width: 1,
		                    color: '#808080'
		                }]
		            },
		            tooltip: {
		                formatter: function() {
		                        return '<b>'+ this.series.name +'</b><br/>'+
		                        this.x +': '+ this.y +'°C';
		                }
		            },
		            legend: {
		                layout: 'vertical',
		                align: 'right',
		                verticalAlign: 'top',
		                x: -10,
		                y: 100,
		                borderWidth: 0
		            },
		            series: [{
		                name: 'Tokyo',
		                data: [7.0, 6.9, 9.5, 14.5, 18.2, 21.5, 25.2, 26.5, 23.3, 18.3, 13.9, 20.6]
		            }, {
		                name: 'New York',
		                data: [-0.2, 0.8, 5.7, 11.3, 17.0, 22.0, 24.8, 24.1, 20.1, 14.1, 8.6, 2.5]
		            }, {
		                name: 'Berlin',
		                data: [-0.9, 0.6, 3.5, 8.4, 13.5, 17.0, 18.6, 17.9, 14.3, 9.0, 3.9, 1.0]
		            }, {
		                name: 'London',
		                data: [3.9, 4.2, 5.7, 8.5, 11.9, 15.2, 17.0, 16.6, 14.2, 10.3, 6.6, 4.8]
		            }]
		        });
		    });
		    
		});
  </script>
  
  <script src="{{asset('js/panel/chart/highcharts.js')}}"></script>
  <script src="{{asset('js/chart/modules/exporting.js')}}"></script>
  
  <!-- ***** Jquery Chart ***** -->  
  
<div class="grid_12" style="margin: 20px 0 0 0;">
	
  <H1><B>Findtheroom Panel</B></H1>
  <hr>
  <p>Welcome to Visual Admin, a slick admin template for almost any project - front or back end. It's based on the 960 grid system which means you can have almost endless layout possibiliites as well as those that come ready to use straight out the box. Please have a click around and feel free to check out the screenshots.</p>
  <div id="chartcontainer" class="grid_12 marginB_20" style="height: 400px; margin: 0 auto"></div>
  <div class="clear"></div>
  <hr>
</div><!-- End #wrap -->

